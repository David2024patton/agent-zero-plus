# ====================================================================
# Agent 0+ Environment Configuration
# ====================================================================
# Copy this file to usr/.env and fill in your credentials.
# The usr/ directory is gitignored â€” your secrets stay local.
#
# Agent Zero will auto-create usr/.env on first run if it doesn't exist.
# You can also configure most of these through the Web UI under Settings.
# ====================================================================

# --- Authentication ---
# Leave empty for no login page, or set to require authentication
AUTH_LOGIN=
AUTH_PASSWORD=

# --- LLM Provider API Keys ---
# Fill in the providers you want to use. At least one is required.
API_KEY_GOOGLE=
API_KEY_OPENAI=
API_KEY_ANTHROPIC=
API_KEY_MISTRAL=
API_KEY_OPENROUTER=
API_KEY_GROQ=
API_KEY_DEEPSEEK=
API_KEY_OLLAMA=
API_KEY_LM_STUDIO=
API_KEY_NVIDIA=
API_KEY_HUGGINGFACE=
API_KEY_ZAI=
API_KEY_ZAI_CODING=
API_KEY_XAI=
API_KEY_SAMBANOVA=
API_KEY_VENICE=
API_KEY_COMETAPI=
API_KEY_MOONSHOT=
API_KEY_AZURE=
API_KEY_BEDROCK=
API_KEY_GITHUB_COPILOT=
API_KEY_OTHER=

# --- Tool-Specific API Keys ---
# These are used by the 30+ custom tools in Agent 0+.
# Only fill in what you need.

# Google Gemini (used by gemini_tool)
GEMINI_API_KEY=

# Mistral AI (used by mistral_tool)
MISTRAL_API_KEY=

# HuggingFace (used by huggingface_tool)
HUGGINGFACE_TOKEN=

# GitHub (used by github_tool)
GITHUB_PERSONAL_ACCESS_TOKEN=

# Hostinger (used by hostinger_tool)
HOSTINGER_API_TOKEN=

# Notion (used by notion_tool)
NOTION_API_KEY=

# Trello (used by trello_tool)
TRELLO_API_KEY=
TRELLO_TOKEN=

# --- Email Configuration ---
# Used by email_tool for sending/receiving emails
EMAIL_ADDRESS=
EMAIL_PASSWORD=
EMAIL_SMTP_HOST=smtp.titan.email
EMAIL_SMTP_PORT=465
EMAIL_IMAP_HOST=imap.titan.email
EMAIL_IMAP_PORT=993

# --- Messaging Plugins ---
# Discord bot (plugins/discord_channel)
DISCORD_BOT_TOKEN=

# Telegram bot (plugins/telegram_channel)
TELEGRAM_BOT_TOKEN=

# --- Optional ---
ANONYMIZED_TELEMETRY=false
